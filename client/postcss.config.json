{
  "plugins": [
    "postcss-nested",
    "postcss-flexbugs-fixes",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "custom-properties": false
        }
      }
    ],
    [
      "postcss-custom-media",
      {
        "importFrom": [
          {
            "customMedia": { "--t": "(min-width: 980px)" }
          },
          {
            "customMedia": { "--d": "(min-width: 1270px)" }
          }
        ]
      }
    ]
  ]
}
